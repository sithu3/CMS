<script type="text/javascript">
 $(document).ready(()=>{
            if(<%= patient.married == 'Single'%>)
            $("#select").val('Single');
            else if(<%= patient.married == 'Married'%>)
              $("#select").val('Married');
              else if(<%= patient.married == 'Other'%>)
                $("#select").val('Other');



                if(<%= patient.gender == 'Male'%>)
                $("#gender").val('Male');
                else if(<%= patient.gender == 'Female'%>)
                  $("#gender").val('Female');


                  if(<%= patient.blood == 'A'%>)
                  $("#blood").val('A');
                  else if(<%= patient.blood == 'B'%>)
                    $("#blood").val('B');
                    else if(<%= patient.blood == 'O'%>)
                      $("#blood").val('O');
                      else if(<%= patient.blood == 'AB'%>)
                        $("#blood").val('AB');


              if(<%= patient.gender == 'Male'%>)
                $("#radio_male").attr('checked', true);
                else   if(<%= patient.gender == 'Female'%>)
                    $("#radio_female").attr('checked', true);
        });
</script>

